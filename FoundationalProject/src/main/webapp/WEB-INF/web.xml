<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>FrontController</servlet-name>
		<servlet-class>com.jean.web.FrontController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FrontController</servlet-name>
		<url-pattern>/register</url-pattern>
		<url-pattern>/login</url-pattern>
		<url-pattern>/users/*</url-pattern>
		<url-pattern>/reimbursement/*</url-pattern>
		<url-pattern>/user/add</url-pattern>
		<url-pattern>/user/delete</url-pattern>
		<url-pattern>/user/update</url-pattern>
		<url-pattern>/users/view/employees</url-pattern>
		<url-pattern>/reimbursements/view/pending</url-pattern>
		<url-pattern>/users/view/all</url-pattern>
		<url-pattern>/reimbursement/submit</url-pattern>
		<url-pattern>/error</url-pattern>
	</servlet-mapping>
</web-app>